                "require": {
                    "php": ">=5.2.0",
                    "ext-xml": "*"
                },
                "autoload": {
                    "psr-0": {
                        "PHPExcel": "Classes/"
                    }
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        },
        {
            "type": "composer",
            "url": "https://packagist.org/"
