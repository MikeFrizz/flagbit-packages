                    "sensio/framework-extra-bundle": ">=2.0"
                },
                "autoload": {
                    "psr-0": { "Ps\\PdfBundle": "" }
                },
                "target-dir": "Ps/PdfBundle"
            }
        },
        {
            "type": "package",
            "package": {
                "name": "phpoffice/phpexcel",
                "version": "0.0.0-patch0",
